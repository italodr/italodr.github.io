{
  "name": "italodr",
  "description": "personal website",
  "version": "0.0.1",
  "keywords": ["italodr", "resume", "cv", "react", "create-react-app"],
  "license": "MIT",
  "private": false,
  "repository": "git@github.com:italodr/italodr.github.io.git",
  "homepage": "http://italodr.github.io",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "styled-components": "^3.1.6",
    "styletron-react": "^3.0.4"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build && mv build docs",
    "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
    "eject": "cross-env NODE_PATH=src react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "echo 'italodr.com' > ./build/CNAME && gh-pages -d build"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "gh-pages": "^1.1.0"
  }
}
